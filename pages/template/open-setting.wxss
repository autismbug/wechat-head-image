.opensetting {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  left: 0px;
  background: #000;
  opacity: 0.7;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.opensetting-dialog {
  position: fixed;
  width: 270px;
  padding: 20px;
  border-radius: 10px;
  z-index: 9999;
  background: #f9f9f9;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  top: 40%;
}

.opensetting-title {
  color: #000;
  text-align: center;
}

.opensetting-button {
  margin-top: 20px;
  background: #ccc;
  margin-left: 10px;
  margin-right: 10px;
}